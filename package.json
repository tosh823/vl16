{
  "name": "vl16",
  "version": "1.0.0",
  "description": "VL",
  "main": "index.js",
  "scripts": {
    "build": "node_modules/.bin/browserify -t [ babelify --presets [ react ] --extensions .jsx] src/client/app.js -o build/bundle.js",
    "watch": "node_modules/.bin/watchify -t [ babelify --presets [ react ] --extensions .jsx] src/client/app.js -o build/bundle.js --debug --verbose",
    "serve": "node_modules/.bin/http-server ./build --cors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tosh823/vl16.git"
  },
  "keywords": [
    "threejs"
  ],
  "author": "Ilya Minyaev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tosh823/vl16/issues"
  },
  "homepage": "https://github.com/tosh823/vl16#readme",
  "dependencies": {
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "browserify-shim": "^3.8.12",
    "http-server": "^0.9.0",
    "watchify": "^3.7.0"
  },
  "browser": {
    "three": "./src/client/libs/three.js",
    "OrbitControls": "./src/client/libs/OrbitControls.js",
    "stats": "./src/client/libs/stats.min.js",
    "dat.GUI": "./src/client/libs/dat.gui.min.js"
  },
  "browserify": {
    "transform": "browserify-shim"
  },
  "browserify-shim": {
    "three": "THREE",
    "dat.GUI": "dat"
  }
}
